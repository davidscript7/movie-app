{
  "expo": {
    "name": "MovieFlix",
    "slug": "mobile_movie_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "_internal": {
      "projectRoot": "./",
      "isDebug": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      ["nativewind/babel", {
        "input": "./globals.css",
        "configPath": "./tailwind.config.js"
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}